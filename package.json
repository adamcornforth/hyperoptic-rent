{
  "name": "hyperoptic-rent",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon src/server.js --watch routes/ --exec babel-node & webpack-dev-server --host 0.0.0.0 --inline --progress --watch --hot",
    "build": "rm -rf dist && mkdir dist && babel src/ -d dist/ && webpack && cp -r src/views dist/views && cp -r src/static/css dist/static/css",
    "serve": "NODE_ENV=prod node ./dist/server.js",
    "postinstall": "npm run build && NODE_ENV=prod PORT=8080 nodemon ./dist/server.js"
  },
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^2.0.0",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "fuse.js": "^2.5.0",
    "google-map-react": "^0.21.5",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.3.2",
    "react-router": "^2.6.1",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.13.3",
    "whatwg-fetch": "^2.0.1",
    "x-ray": "^2.3.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "react-pure-render": "^1.0.2",
    "react-controllables": "^0.6.0",
    "nodemon": "^1.11.0"
  },
  "devDependencies": {
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "http-server": "^0.9.0",
    "react-hot-loader": "^1.3.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
